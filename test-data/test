window.onscroll = function () {
    var winScroll = document.documentElement.scrollTop || document.body.scrollTop;
    if(winScroll >= 700){
        $('#goTop').show();
    }else{
        $('#goTop').hide();
    }

    var ch = document.documentElement.clientHeight;//窗口的高度
    var at = document.getElementById("leftShare").offsetTop;//元素离页面顶部的高度
   
    if(winScroll >= (at - ch + 205)){
      
      $('#leftScroll').show();

    }else{
      $('#leftScroll').hide();
    }
}

 </script>
